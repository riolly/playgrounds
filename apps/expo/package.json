{
  "name": "@playground/expo",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "clean": "git clean -xdf .expo .turbo node_modules",
    "dev": "expo start",
    "dev:android": "expo start --android",
    "dev:ios": "expo start --ios",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "get-branch-name": "git rev-parse --abbrev-ref HEAD",
    "build:dev": "BUILD_TARGET=development eas build --profile=development --platform=android --local --output=\".apk/beloved_one-v$npm_package_version(dev)\".apk",
    "build:prev": "sh -c 'BRANCH_NAME=$(npm run get-branch-name --silent) && BUILD_TARGET=preview BRANCH_NAME=$BRANCH_NAME eas build --profile=preview --platform=android --local --output=\".apk/beloved_one-v$npm_package_version($BRANCH_NAME).apk\"'",
    "build:prod": "BUILD_TARGET=production eas build --profile=production --platform=android --local --output=\".apk/beloved_one-v$npm_package_version\".aab"
  },
  "dependencies": {
    "@bacons/text-decoder": "^0.0.0",
    "@expo/metro-config": "^0.18.8",
    "@shopify/flash-list": "1.6.4",
    "@tanstack/react-query": "^5.49.2",
    "@trpc/client": "11.0.0-rc.441",
    "@trpc/react-query": "11.0.0-rc.441",
    "@trpc/server": "11.0.0-rc.441",
    "expo": "~51.0.20",
    "expo-constants": "~16.0.2",
    "expo-dev-client": "~4.0.20",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.18",
    "expo-secure-store": "^13.0.2",
    "expo-splash-screen": "~0.27.5",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "^3.0.7",
    "expo-web-browser": "^13.0.3",
    "nativewind": "~4.0.36",
    "react": "catalog:react18",
    "react-dom": "catalog:react18",
    "react-native": "~0.74.3",
    "react-native-css-interop": "~0.0.34",
    "react-native-gesture-handler": "~2.16.2",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "~4.10.1",
    "react-native-screens": "~3.31.1",
    "superjson": "2.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/runtime": "^7.24.7",
    "@playground/api": "workspace:*",
    "@playground/eslint-config": "workspace:*",
    "@playground/prettier-config": "workspace:*",
    "@playground/tailwind-config": "workspace:*",
    "@playground/tsconfig": "workspace:*",
    "@types/babel__core": "^7.20.5",
    "@types/react": "catalog:react18",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "tailwindcss": "^3.4.4",
    "typescript": "catalog:"
  },
  "prettier": "@playground/prettier-config"
}
